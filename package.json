    "description": "",
    "version": "0.0.1",
    "publisher": "softwaredotcom",
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "test": "node ./out/test/runTest.js"
    },
    "engines": {
        "vscode": "^1.43.0"
    },
    "preview": true,
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#384356",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/swdotcom/swdc-vscode-100-days-of-code"
    },
    "license": "SEE LICENSE IN LICENSE",
    "keywords": [
        "productivity"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "DoC.LaunchSoftwareWebsite",
                "title": "Goes to Software.com"
            },
            {
                "command": "DoC.ToggleStatusbar",
                "title": " "
            },
            {
                "command": "DoC.BlankBlueLine",
                "title": " "
            },
            {
                "command": "DoC.goalsCMD",
                "title": "Goals"
            },
            {
                "command": "DoC.diaryAdd",
                "title": "Add Diary Page"
            },
            {
                "command": "DoC.diaryCMD",
                "title": "Diary"
            },
            {
                "command": "DoC.milestonesCMD",
                "title": "Milestones"
            },
            {
                "command": "DoC.shareCMD",
